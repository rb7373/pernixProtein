$bg-green = #009688
$bg-blue = #3949ab

.bg-green
  background-color $bg-green

.bg-blue
  background-color $bg-blue

.whiteframe
  max-width 300px
  width 80%
  background #fff
  margin 20px
  padding 20px

.fill-95
  width 95%

.content-frame
  padding-bottom 16px



.titleGoals
  margin-top 0px
  font-weight bold

.margin-sm
  margin-bottom 85px

.objective-button a {
  font-weight bold
  display: table;
  padding: 10px;
  margin-top 10px
  width: 100%;
  height inherit
  background-color: #222;
  text-decoration: none;
  text-align: left;
  vertical-align: top;
  color: #fff;
  overflow: hidden;
  -webkit-transition-property: background;
  -webkit-transition-duration: 0.4s;
  -webkit-transition-timing-function: ease-out;
  transition-property: background;
  transition-duration: 0.4s;
  transition-timing-function: ease-out;
}
