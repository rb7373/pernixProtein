.container-fluid
  .row
    .col-xs-10.col-sm-6.col-md-4.col-lg-4
      nav#menu_box
      ul.menu(ng-mouseleave="hideOptions()")
        li
          a.topMenuOption(href="#", ng-mouseenter="showOptions()")
            span
              ng-md-icon(icon="add", size="28", ng-show="!showMenu")
              ng-md-icon(icon="remove", size="28", ng-show="showMenu")
            span.titleMenu
              | {{navigation.getCurrentStateName()}}
        li.subMenu.fadeMenu(ng-show="showMenu", ng-repeat="section in sections")
          a(href="#")
            span.subTitleMenu
              | {{section.name}}
          ul
            li
              a(href="#")
                span.subTitleItemMenu
                  | Objectives
            li
              a(href="#")
                span.subTitleItemMenu
                  | Animation
            li
              a(href="#")
                span.subTitleItemMenu
                  | Practice Session
        li
          a.topMenuOption.fadeMenu(ng-show="showMenu")
            div
              span.footerSubTitleMenu
                | Voet, Voet & Pratt
            div
              span.footerTitleMenu
                | Fundamentals of Biochemistry
